    "def replacePI(string):\n",
    "  if(len(string) == 1):\n",
    "    return string\n",
    "  if(string[0] == \"p\" and string[1] == \"i\"):\n",
    "    return \"3.14\" + replacePI(string[2:])\n",
    "  return string[0] + replacePI(string[1:])\n",
    "\n",
    "string = \"3f43pie32eppi3.14pr\"\n",
    "replacePI(string)"
   ]
